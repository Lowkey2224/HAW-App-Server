<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="HawServer">
		<class>com.hawserver.mensaplan.model.MenuItem</class>
		<class>com.hawserver.schnitzeljagd.model.Schnitzeljagd</class>
		<class>com.hawserver.schnitzeljagd.model.Ziel</class>
		<properties>
			<property name="javax.persistence.jdbc.password" value="hawserver"/>
			<property name="javax.persistence.jdbc.user" value="hawserver"/>
			<property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
			<property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/hawserver"/>
		</properties>
	</persistence-unit>
</persistence>
